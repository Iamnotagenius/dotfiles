{
    "coc.source.vimtex.enable": true,
    "coc.preferences.formatOnSave": true,
    "semanticTokens.enable": true,
    "semanticTokens.filetypes": [
        "*"
    ],
    "semanticTokens.combinedModifiers": [
        "deprecated"
    ],
    "semanticTokens.highlightPriority": 2048,
    "semanticTokens.incrementTypes": [
        "variable",
        "string",
        "parameter"
    ],
    "clangd.path": "~/.config/coc/extensions/coc-clangd-data/install/13.0.0/clangd_13.0.0/bin/clangd",
    "clangd.arguments": [
        "--clang-tidy",
        "--header-insertion=never",
        "--background-index"
    ],
    "sql.database": "mysql",
    "sql.lintOnChange": true,
    "links.tooltip": true,
    "rpc.detailsViewing": "📂 {workspace_folder} {problems}",
    "rpc.detailsEditing": "📂 {workspace_folder} {problems}",
    "rpc.lowerDetailsEditing": "✏️ {file_name} ({current_line}/{total_lines})",
    "rpc.lowerDetailsViewing": "👀 {file_name} ({current_line}/{total_lines})",
    "rpc.detailsIdling": "AFK",
    "rpc.idleText": "🤪",
    "rpc.buttonActiveLabel": "Go check it",
    "rpc.buttonEnabled": true,
    "rpc.buttonInactiveLabel": "Go to my Github",
    "rpc.buttonInactiveUrl": "https://www.youtube.com/watch?v=dQw4w9WgXcQ",
    "rpc.largeImage": "I'm stuck in {LANG}",
    "diagnostic.floatConfig": {
        "winblend": 40,
        "border": true,
        "rounded": true
    },
    "hover.floatConfig": {
        "border": true,
        "rounded": true,
        "winblend": 40
    },
    "diagnostic.infoSign": "",
    "diagnostic.hintSign": "",
    "diagnostic.errorSign": "",
    "diagnostic.warningSign": "",
    "diagnostic.autoRefresh": true,
    "diagnostic.virtualTextLines": 3,
    "diagnostic.refreshOnInsertMode": true,
    "diagnostic.enableMessage": "jump",
    "diagnostic.virtualText": true,
    "diagnostic.virtualTextCurrentLineOnly": false,
    "diagnostic.virtualTextLineSeparator": " | ",
    "diagnostic.virtualTextPrefix": "<- ",
    "Lua.runtime.version": "Lua 5.1",
    "Lua.runtime.nonstandardSymbol": [
        "!="
    ],
    "Lua.color.mode": "Semantic",
    "suggest.virtualText": true,
    "suggest.floatConfig": {
        "border": true,
        "rounded": true,
        "winblend": 40
    },
    "suggest.selection": "recentlyUsed",
    "suggest.triggerAfterInsertEnter": true,
    "suggest.removeDuplicateItems": true,
    "suggest.snippetsSupport": true,
    "suggest.completionItemKindLabels": {
        "keyword": "\uf1de",
        "variable": "\ue79b",
        "value": "\uf89f",
        "operator": "\u03a8",
        "constructor": "\uf0ad",
        "function": "\u0192",
        "reference": "\ufa46",
        "constant": "\uf8fe",
        "method": "\uf09a",
        "struct": "\ufb44",
        "class": "\uf0e8",
        "interface": "\uf417",
        "text": "\ue612",
        "enum": "\uf435",
        "enumMember": "\uf02b",
        "module": "\uf40d",
        "color": "\ue22b",
        "property": "\ue624",
        "field": "\uf9be",
        "unit": "\uf475",
        "event": "\ufacd",
        "file": "\uf723",
        "folder": "\uf114",
        "snippet": "\ue60b",
        "typeParameter": "\uf728",
        "default": "\uf29c"
    },
    "go.enable": true,
    "go.goplsOptions": {
        "semanticTokens": true,
        "importShortcut": "Both"
    },
