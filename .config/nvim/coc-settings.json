{
    "coc.source.vimtex.enable": true,
    "coc.preferences.formatOnSave": true,
    "coc.preferences.formatOnType": true,
    "coc.preferences.bracketEnterImprove": true,
    "coc.preferences.currentFunctionSymbolAutoUpdate": true,
    "semanticTokens.enable": true,
    "semanticTokens.combinedModifiers": [
        "deprecated"
    ],
    "semanticTokens.highlightPriority": 2048,
    "semanticTokens.incrementTypes": [
        "variable",
        "string",
        "parameter"
    ],
    "clangd.path": "~/.config/coc/extensions/coc-clangd-data/install/13.0.0/clangd_13.0.0/bin/clangd",
    "clangd.arguments": [
        "--clang-tidy",
        "--header-insertion=never",
        "--background-index"
    ],
    "sql.database": "postgresql",
    "sql.lintOnChange": true,
    "links.tooltip": true,
    "rpc.enabled": false,
    "rpc.detailsViewing": "üìÇ {workspace_folder} {problems}",
    "rpc.detailsEditing": "üìÇ {workspace_folder} {problems}",
    "rpc.lowerDetailsEditing": "‚úèÔ∏è {file_name} ({current_line}/{total_lines})",
    "rpc.lowerDetailsViewing": "üëÄ {file_name} ({current_line}/{total_lines})",
    "rpc.detailsIdling": "AFK",
    "rpc.idleText": "ü§™",
    "rpc.buttonActiveLabel": "Go check it",
