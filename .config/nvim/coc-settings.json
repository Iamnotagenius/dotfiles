    "rpc.buttonInactiveLabel": "Go to my Github",
    "rpc.buttonInactiveUrl": "https://www.youtube.com/watch?v=dQw4w9WgXcQ",
    "rpc.largeImage": "I'm stuck in {LANG}",
    "signature.enable": true,
    "signature.hideOnTextChange": false,
    "signature.triggerSignatureWait": 200,
    "signature.floatConfig": {
        "winblend": 40,
        "border": true,
        "rounded": true
    },
    "diagnostic.floatConfig": {
        "winblend": 40,
        "border": true,
        "rounded": true
    },
    "hover.floatConfig": {
        "border": true,
        "rounded": true,
        "winblend": 40
    },
    "diagnostic.infoSign": "ÔÅö",
    "diagnostic.hintSign": "ÔÅô",
    "diagnostic.errorSign": "ÔÅó",
    "diagnostic.warningSign": "ÔÅ±",
